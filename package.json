{
  "name": "how-the-west-was-won",
  "version": "1.0.0",
  "description": "A pleasant library full of time-saving mocha wrappers and test utilities",
  "main": "lib/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Koleok/how-the-test-was-won.git"
  },
  "scripts": {
    "start": "webpack && node ./build/build.js",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "debug": "node-inspector & nodemon --debug .",
    "test": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./src/tests/test",
    "test-unit": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./src/tests/unit",
    "test-integration": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./src/tests/integration",
    "watch": "./node_modules/mocha/bin/_mocha --compilers js:babel-core/register ./src/tests/test --watch"
  },
  "author": "Kyle Chamberlain",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-inspector": "^0.12.8",
    "webpack": "^1.12.14"
  }
}
